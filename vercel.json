{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "crons": [
    {
      "path": "/api/syncusers",
      "schedule": "31 13 * * 1"
    },
    {
      "path": "/api/syncusers",
      "schedule": "0 14-20 * * 1-5"
    },
    {
      "path": "/api/syncusers",
      "schedule": "30 20 * * 1-5"
    }
  ],
  "rewrites": [
    {
      "source": "/api/mexc-futures/:path*",
      "destination": "/api/mexc-futures"
    },
    {
      "source": "/api/mexc-spot/:path*",
      "destination": "/api/mexc-spot"
    },
    {
      "source": "/((?!api).*)",
      "destination": "/index.html"
    }
  ],
  "functions": {
    "api/**/*.ts": {
      "runtime": "@vercel/node@5.5.16"
    }
  }
}